er;\r\n    name: string;\r\n    date_utc: string;\r\n    success: boolean;\r\n    links: {\r\n        wikipedia: string;\r\n    }\r\n}\r\n\r\ninterface IData1 {\r\n    id: string;\r\n    flight_number: number;\r\n    name: string;\r\n    date_utc: string;\r\n    success: string;\r\n    wikipedia: string;\r\n}\r\n\r\n\r\nconst List: React.FC<IRouteParams> = ({ match }) => {\r\n\r\n   \r\n    \r\n    const[data, setData] = useState<IData1[]>([]);\r\n\r\n    const [items, setItems] = useState<IData[]>([]);\r\n \r\n    const {type} = match.params;\r\n    const title = useMemo(() => {\r\n        \r\n        if(type === 'next'){\r\n            return ({\r\n                text : 'Próximo Lançamento',\r\n                lineColor:'#F7931B',\r\n                url: '/proximo'\r\n            });\r\n        }\r\n        else if(type === 'last'){\r\n            return ({\r\n                text : 'Último Lançamento',\r\n                lineColor:'#F7931B',\r\n                url: '/ultimo'\r\n            });       \r\n        }\r\n        else if(type === 'upcoming'){\r\n            return ({\r\n                text : 'Próximos Lançamentos',\r\n                lineColor:'#F7931B',\r\n                url: '/proximos'\r\n            });\r\n        }\r\n        else if(type === 'past'){\r\n            return ({\r\n                text : 'Lançamentos Passados',\r\n                lineColor:'#F7931B',\r\n                url: '/passados'\r\n            });\r\n        }\r\n        else {\r\n            return ({\r\n                text : 'Próximo Lançamento',\r\n                lineColor:'#F7931B',\r\n                url: '/proximo'\r\n            });\r\n        }\r\n    },[type]);\r\n\r\n    \r\n    // com Async Await\r\n    useEffect(() => {\r\n        async function getItems() {\r\n        try {\r\n            const {data} = await Api.get(title.url);\r\n            if(Array.isArray(data)){\r\n                //console.log(data);\r\n                await setItems(data); \r\n       